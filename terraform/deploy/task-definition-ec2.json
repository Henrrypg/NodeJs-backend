{
    "executionRoleArn": "arn:aws:iam::083762234070:role/ecsTaskExecutionRole",
    "memory": "512",

    "cpu": "256",
    
    "taskRoleArn": "arn:aws:iam::083762234070:role/ecsTaskExecutionRole",
    "family":"backend", 
    "containerDefinitions": [
        { 
            "name": "backend",
            "image": "url/",
            "cpu": 256,
            "memoryReservation": 512,
          
            "logConfiguration": {
                "logDriver": "awslogs",
            
                "options": {
                    "awslogs-group": "backend",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
           
            "portMappings": [
                {
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "containerPort": 3000
                }
            ],         
            "environment": [],          
            "essential": true,          
            "readonlyRootFilesystem": false
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "bridge",
    "volumes": []
  }